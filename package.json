{
  "name": "@defi-wonderland/lopt",
  "version": "0.0.1",
  "description": "Mocking framework for Solidity contracts",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "test",
    "solidity",
    "hardhat",
    "mocking",
    "fakes",
    "mocks",
    "chai"
  ],
  "homepage": "https://github.com/defi-wonderland/lopt#readme",
  "bugs": {
    "url": "https://github.com/defi-wonderland/lopt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/defi-wonderland/lopt.git"
  },
  "license": "MIT",
  "author": "DeFi Wonderland",
  "contributors": [
    "kiwi745 (https://github.com/kiwi745)"
  ],
  "main": "src/index",
  "types": "src/index",
  "scripts": {
    "compile": "hardhat compile",
    "test": "cross-env TEST=true mocha 'test/unit/**/*.spec.ts'",
    "lint:check": "cross-env solhint 'contracts/**/*.sol' 'interfaces/**/*.sol' && cross-env prettier --check './**'",
    "lint:fix": "sort-package-json && cross-env prettier --write './**' && cross-env solhint --fix 'contracts/**/*.sol' 'interfaces/**/*.sol'",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "standard-version",
    "docs:install": "pip install -r docs/requirements.txt",
    "docs:watch": "sphinx-autobuild -a docs/source docs/_build/html --watch docs/source"
  },
  "resolutions": {
    "@ethereumjs/block": "3.2.1",
    "@ethereumjs/blockchain": "5.2.1",
    "@ethereumjs/common": "2.2.0",
    "@ethereumjs/tx": "3.1.4",
    "@ethereumjs/vm": "5.3.1"
  },
  "dependencies": {
    "@nomiclabs/ethereumjs-vm": "^4.2.2",
    "@openzeppelin/contracts": "4.1.0",
    "diff": "^5.0.0",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@typechain/ethers-v5": "7.0.1",
    "@typechain/hardhat": "2.0.2",
    "@types/chai": "4.2.18",
    "@types/diff": "^5.0.1",
    "@types/lodash": "4.14.170",
    "@types/mocha": "8.2.2",
    "@types/node": "15.12.2",
    "chai": "4.3.4",
    "cross-env": "7.0.3",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.4.1",
    "hardhat": "2.3.0",
    "hardhat-preprocessor": "0.1.4",
    "husky": "6.0.0",
    "inquirer": "8.1.0",
    "lodash": "4.17.21",
    "mocha": "9.0.0",
    "pinst": "2.1.6",
    "prettier": "2.3.1",
    "prettier-plugin-solidity": "1.0.0-beta.13",
    "solc-0.8": "npm:solc@^0.8.5",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-docgen": "0.5.13",
    "sort-package-json": "1.50.0",
    "standard-version": "9.3.0",
    "ts-node": "10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typechain": "5.1.1",
    "typescript": "4.3.2"
  }
}
